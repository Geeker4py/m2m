<html lang="en">
<head>
    {{> head}}
    <title>{{article.meta.title}} - {{site.title}}</title>  
</head>
<body>
    <div class="book without-animation with-summary font-size-2 font-family-1" data-level="0" data-basepath="." data-revision="Fri Dec 11 2015 14:42:31 GMT+0800 (CST)">
        
        {{> summary}}

        <div class="book-body">
            <div class="body-inner">
                {{> header}}

                <div class="page-wrapper">
                    <div class="page-inner">
                        <section class="normal" id="section">
                            <h1 id="three-principles">{{article.meta.title}}</h1>
                            {{{article.body_html}}}
                        </section>
                    </div>
                </div>
            </div>
        </div>
    </div>

    {{>footer}}
</body>
</html>